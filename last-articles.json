---
layout: null
permalink: /last-articles.json
---
[
	 {% for post in site.categories['articles'] limit:10 %}
    {
		"title": "{{ post.title }}",
		"tags": [
			 {% for tag in post.tags %}
				"{{ tag }}"
				{% if forloop.last == false %},{% endif %}
			{% endfor %}
		],
		"description": "{{ post.description }}"
	}{% if forloop.last == false %},{% endif %}
   {% endfor %}
	
]